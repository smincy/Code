import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int tc, sum, input;

		Scanner sc = new Scanner(System.in);

		tc = sc.nextInt();

		for (int i = 0; i < tc; i++) {
			sum = 0;

			for (int j = 0; j < 10; j++) {
				input = sc.nextInt();

				if (input % 2 == 1) {
					sum += input;
				}
			}

			System.out.printf("#%d %d\n", i + 1, sum);
		}
	}
}
